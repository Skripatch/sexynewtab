{
	"manifest_version": 2,
	"name": "Sexy New Tab",
	"description":"__MSG_descript__",
	"permissions": ["tabs", "bookmarks", "http://*/*", "https://*/*", "storage", "topSites", "contextMenus", "history"],
	"optional_permissions": ["unlimitedStorage"],
	"background": {
		"scripts":["background.js", "MD5.js"]
	},
	"chrome_url_overrides": {
		"newtab": "layout.html"
	},
	"content_scripts": [ {
		"js": [ "content_script.js" ],
		"matches": [ "http://*/*", "https://*/*" ],
		"run_at": "document_end"
	} ],
	"icons": {
		"128": "icons/128.png",
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png"
	},
	"update_url": "http://clients2.google.com/service/update2/crx",
	"default_locale": "en",
	"version": "0.7"
}
